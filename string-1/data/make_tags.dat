make_tags
make_tags('i', 'Yay') → '<i>Yay</i>'
make_tags('i', 'Hello') → '<i>Hello</i>'
make_tags('cite', 'Yay') → '<cite>Yay</cite>'
make_tags('address', 'here') → '<address>here</address>'
make_tags('body', 'Heart') → '<body>Heart</body>'
make_tags('i', 'i') → '<i>i</i>'
make_tags('i', '') → '<i></i>'
